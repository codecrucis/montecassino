views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("Payne McDowell")
        - file.name != "Table of Contents.base"
    order:
      - file.folder
      - file.name
    sort:
      - property: file.folder
        direction: ASC
      - property: file.name
        direction: ASC
